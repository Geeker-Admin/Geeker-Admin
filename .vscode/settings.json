{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.stylelint": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": ["vue", "javascript", "typescript", "typescriptreact"],
  "i18n-ally.sortKeys": true,
  "i18n-ally.keepFulfilled": true,
  "i18n-ally.localesPaths": ["src/languages/modules"],
  "i18n-ally.keystyle": "nested",
  "i18n-ally.sourceLanguage": "zh",
  "i18n-ally.displayLanguage": "zh",
  "i18n-ally.enabledFrameworks": ["vue"],
  "i18n-ally.namespace": false, // 你的项目没有用 namespace，建议设为 false
  "i18n-ally.pathMatcher": "{locale}.ts", // 明确指定 ts 文件
  "i18n-ally.enabledParsers": ["ts"], // 强制启用 ts 解析
  "stylelint.enable": true,
  "stylelint.validate": ["css", "less", "postcss", "scss", "vue", "sass", "html"],
  "files.eol": "\n",
  "todo-tree.regex.regex": "(//)\\s*($TAGS).*(\\n\\s*//\\s{2,}.*)*",
  "todo-tree.general.tags": ["BUG", "HACK", "FIXME", "TODO", "XXX", "[ ]", "[x]", "todo"],
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "cSpell.words": [
    "AMAP",
    "apng",
    "axios",
    "Biao",
    "brotli",
    "cascader",
    "commitlint",
    "contentleft",
    "contentright",
    "CSDN",
    "daterange",
    "datetimerange",
    "echarts",
    "fangda",
    "geeker",
    "Gitee",
    "hexs",
    "huiche",
    "iconfont",
    "juejin",
    "liquidfill",
    "longpress",
    "maicss",
    "monthrange",
    "nprogress",
    "officedocument",
    "openxmlformats",
    "Pageable",
    "persistedstate",
    "pinia",
    "pjpeg",
    "Prefixs",
    "screenfull",
    "sortablejs",
    "sousuo",
    "spreadsheetml",
    "styl",
    "stylelint",
    "stylelintignore",
    "stylelintrc",
    "suoxiao",
    "truetype",
    "tuichu",
    "unplugin",
    "unref",
    "VITE",
    "vuedraggable",
    "vueuse",
    "Vuex",
    "wangeditor",
    "xiala",
    "xiaoxi",
    "Yahei",
    "yiwen",
    "zhongyingwen",
    "zhuti"
  ],
  "editor.rulers": [120]
}
